	<queryString>
		<![CDATA[SELECT
     parc.`codi` AS parc_codi,
     parc.`nom` AS parc_nom,
     parc.`url_foto` AS parc_url_foto,
     zona.`numero` AS zona_numero,
     zona.`parc` AS zona_parc,
     zona.`nom` AS zona_nom,
     atraccio.`codi` AS atraccio_codi,
     atraccio.`zona` AS atraccio_zona,
     atraccio.`capacitat_maxima_ronda` AS atraccio_capacitat_maxima_ronda,
     atraccio.`descripcio_html` AS atraccio_descripcio_html,
     atraccio.`nom` AS atraccio_nom,
     atraccio.`temps_per_ronda` AS atraccio_temps_per_ronda,
     atraccio.`url_foto` AS atraccio_url_foto,
     atraccio.`clients_en_cua` AS atraccio_clients_en_cua,
     atraccio.`alsada_minima_amb_acompanyant` AS atraccio_alsada_minima_amb_acompanyant,
     atraccio.`alsada_minima` AS atraccio_alsada_minima,
     atraccio.`estat_operatiu` AS atraccio_estat_operatiu,
     atraccio.`incidencia` AS atraccio_incidencia,
     info_utilitzacio.`passi` AS info_utilitzacio_passi,
     info_utilitzacio.`atraccio` AS info_utilitzacio_atraccio,
     info_utilitzacio.`numero_usos` AS info_utilitzacio_numero_usos,
     info_utilitzacio.`tipus_acces` AS info_utilitzacio_tipus_acces,
     entrada_parc.`entrada` AS entrada_parc_entrada,
     entrada_parc.`parc` AS entrada_parc_parc
     entrada_adult.`id` AS entrada_adult_id
     entrada_senior.`id` AS entrada_senior_id
     entrada_discapacitat.`id` AS entrada_discapacitat_id
FROM
     `parc` parc LEFT OUTER JOIN `zona` zona ON parc.`codi` = zona.`parc`
     LEFT OUTER JOIN `atraccio` atraccio ON zona.`numero` = atraccio.`zona`
     LEFT OUTER JOIN `info_utilitzacio` info_utilitzacio ON atraccio.`codi` = info_utilitzacio.`atraccio`
     LEFT OUTER JOIN `entrada_parc` entrada_parc ON parc.`codi` = entrada_parc.`parc`
     LEFT OUTER JOIN `entrada` entrada_adult ON entrada_parc.`entrada` = entrada_adult.`id`
     LEFT OUTER JOIN `entrada` entrada_senior ON entrada_parc.`entrada` = entrada_senior.`id`
     LEFT OUTER JOIN `entrada` entrada_discapacitat ON entrada_parc.`entrada` = entrada_discapacitat.`id`
WHERE entrada_adult.categoria = 'ADULT']]>
	</queryString>
